class Precedence {
	precedence() : Precedence{{
		3+3*5+3-(3*5+1)+3*(3-3<1)*3=5-8;
		let x:Int<-3, y:Int in x*5+213+3*5+3-(3*5+1)+3*(3-3<1)*3=5-8;
	}};
};

